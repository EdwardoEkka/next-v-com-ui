{
  "extends": [
    "eslint:recommended",
    "plugin:@next/next/recommended"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "requireConfigFile": false
  },
  "rules": {
    "no-console": "warn",
    "semi": ["error", "always"]
  }
}
